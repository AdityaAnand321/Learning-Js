Convert Js Object to JSON string

let obj={name: aditya, age: 25}
let jsonStr=JSON.stringify(obj);
// Convert JSON string to Js Object

let jsonStr='{"name":"aditya","age":25}';
let obj=JSON.parse(jsonStr);

